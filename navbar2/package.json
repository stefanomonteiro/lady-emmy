{
  "name": "lady-emmy",
  "version": "1.0.0",
  "description": "Lady Emmy Website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/noprefix.style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",

    "prefix:css":
      "postcss --use autoprefixer -b 'last 10 versions' css/noprefix.style.css -o css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanomonteiro/lady-emmy.git"
  },
  "author": "Stefano Monteiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stefanomonteiro/lady-emmy/issues"
  },
  "homepage": "https://github.com/stefanomonteiro/lady-emmy#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0"
  },
  "dependencies": {}
}
